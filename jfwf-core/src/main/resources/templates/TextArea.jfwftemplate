<div class="container">
    <div class="row">
        <div class="mb-3">
            <label th:for="${id}" th:text="${name}" class="form-label"></label>
            <textarea type="text" class="form-control" th:id="${id}" th:attr="key-value='' + ${keyValue} + ''" th:text="${value}"
                      th:style="'height: ' + ${height}"></textarea>
        </div>
    </div>
</div>
